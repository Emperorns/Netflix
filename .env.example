# MongoDB connection string
MONGODB_URI=mongodb+srv://<username>:<password>@<cluster>.mongodb.net/telegram-netflix?retryWrites=true&w=majority

# Telegram Bot Token (from BotFather)
TELEGRAM_BOT_TOKEN=

# Telegram Channel ID (e.g. @channelname or -100123456789)
TELEGRAM_CHANNEL_ID=

# Server port (optional, defaults to 5000)
PORT=5000

# Node environment
NODE_ENV=development
